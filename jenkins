pipeline {
agent any
stages {
 stage("Code Checkout from GitLab") {
  steps {
   git branch: 'main',
    credentialsId: 'gitlab_access_token',
    url: 'https://github.com/lakeshsharma/nCompass/edit/main/jenkins'
  }
 }
   stage('Code Quality Check via SonarQube') {
   steps {
       script {
       def scannerHome = tool 'sonarqube';
           withSonarQubeEnv("sonarqube-container") {
           sh "${tool("sonarqube")}/bin/sonar-scanner \
           -Dsonar.projectKey=test-node-js \
           -Dsonar.sources=. \
           -Dsonar.css.node=. \
           -Dsonar.host.url=https://quality.aristocrat.com/projects \
           -Dsonar.login=0047c404603d306e5740e973d4c57c218e31b9e5"
               }
           }
       }
   }
}
}
